if BOARD_TWR_KE18F

config BOARD_TWR_KE18F_FLEXIO_CLKOUT
	bool "Enable CLKOUT signal on FlexIO header"
	depends on PINMUX_MCUX_PORTE
	help
	  Enable the CLKOUT signal on FlexIO header pin 7 (PTE10).

endif #BOARD_TWR_KE18F
